@import '../../asset/font/iconfont.wxss';
.navbar {
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
  flex-shrink: 0;
}
.nav-leftCapsule{
  display: flex;
  width: 174rpx;
  height: 56rpx;
  border:1px solid rgba(0,0,0,0.08);
  border-radius:56px;
  overflow: hidden;
  position: absolute;
  left: 10px;
  z-index: 99;
}
.goback,.goHome{
  width: 50%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
}
.border-r{
  border-right:0.05rpx solid rgba(0,0,0,0.08);
}
.border-l{
  border-left:0.05rpx solid rgba(0,0,0,0.08);
}
.navbar-title {
  width: 100%;
  box-sizing: border-box;
  padding-left: 115px;
  padding-right: 115px;
  height: 32px;
  line-height: 32px;
  text-align: center;
  position: absolute;
  left: 0;
  z-index: 10;
  font-size: 36rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}